{
    "name": "coleshirley/filament-package-tests-lifecycle-example",
    "description": "This is my package filament-package-tests-lifecycle-example",
    "keywords": [
        "coleshirley",
        "laravel",
        "filament-package-tests-lifecycle-example"
    ],
    "homepage": "https://github.com/coleshirley/filament-package-tests-lifecycle-example",
    "license": "MIT",
    "authors": [
        {
            "name": "Cole",
            "email": "cole.shirley@artificertech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^2.15",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Coleshirley\\FilamentPackageTestsLifecycleExample\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coleshirley\\FilamentPackageTestsLifecycleExample\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {},
    "minimum-stability": "dev",
    "prefer-stable": true
}
